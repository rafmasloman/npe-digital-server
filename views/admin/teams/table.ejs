<table id="" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama</th>
      <th>Role</th>
      <th>Profile Picture</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% teams.forEach((team, i) => { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= team.name %></td>
      <td><%= team.role.name %></td>
      <td>
        <img
          src="/uploads/teams/<%= team.image %>"
          class="img-fluid img-rounded"
          style="width: 150px; height: 150px"
        />
      </td>
      <td>
        <form
          action="/teams/delete/<%= team.id %>?_method=DELETE"
          method="post"
        >
          <a href="/teams/edit/<%= team.id %>" class="btn btn-warning mr-3"
            >Edit</a
          >
          <button class="btn btn-danger">Delete</button>
        </form>
      </td>
    </tr>
    <% }); %>
